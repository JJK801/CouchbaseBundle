{
    "name": "toiine/couchbasebundle",
    "description": "Symfony2 bundle to manipulate Couchbase Documents.",
    "version": "0.1.0",
    "homepage": "https://github.com/toiine/CouchbaseBundle",
    "type": "symfony-bundle",
    "keywords": ["couchbase"],
    "time": "2013-07-24",
    "license": "GPL-3.0",
