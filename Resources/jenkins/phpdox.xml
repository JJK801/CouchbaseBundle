<?xml version="1.0" ?>
<phpdox xmlns="http://xml.phpdox.net/config" silent="false">
    <project name="CouchbaseBundle" source="." workdir="build/phpdox">
        <collector publiconly="false">
            <include mask="*.php" />
            <exclude mask="*Autoload.php" />
            <exclude mask="**vendor**"/>
            <exclude mask="**Tests**"/>
            <exclude mask="**DependencyInjection**"/>
        </collector>

        <generator output="build">
            <build engine="html" enabled="true" output="api"/>
        </generator>
    </project>
</phpdox>
